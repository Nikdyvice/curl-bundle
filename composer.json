{
    "name": "nikdyvice/curl-bundle",
    "description": "Symfony bundle wrapper for php-curl-class",
    "type": "symfony-bundle",
    "keywords": ["curl"],
    "require": {
        "php": ">=5.5.3",
        "symfony/symfony": "3.*",
        "php-curl-class/php-curl-class": "^7.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nikdyvice",
            "email": "nikdyvice@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Nikdyvice\\CurlBundle": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    }
}
